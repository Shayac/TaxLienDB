<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css/bootstrap.css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
    <link rel="stylesheet" href="~/Content/css/Print.css" type="text/css" media="print"/>
</head>
    <body>
        <header>
            
            
            <div class="navbar navbar-default noprint" role="navigation">

                <div class="container">
                    <div class="navbar-header">
                        <a class="navbar-brand" href="/Home">Tax Lien Tracker</a>
                        <ul class="nav navbar-nav">
                            <li>@Html.ActionLink("Purchase", "Purchase", "Properties")
                            </li>
                            <li>@Html.ActionLink("Subsequents","Index","Subsequents")</li>
                            <li>@Html.ActionLink("Redemptions","Index","Redemptions")</li>
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#">Reports <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Outstanding Liens</a></li>
                                    <li><a href="/Reports">Redeemed Liens</a></li>
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#">Settings<b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="/settings/counties">Counties</a></li>
                                    
                                </ul>
                            </li>
                        </ul>

                    </div>

                </div>

            </div>
        </header>
        
        <div class="container">
            @RenderBody()
        </div>
        
        @Scripts.Render("~/bundles/jquery", "~/Scripts/Bootstrap.js")
        @RenderSection("scripts", required: false)
    </body>
</html>
