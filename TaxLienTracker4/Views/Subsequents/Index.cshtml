@using DataLayer
@model System.Collections.Generic.IEnumerable<DataLayer.County>

<div class="row">
   
    @Html.Action("CountyPicker","Home")
   
    <div class="col-md-3 col-md-offset-1">
    <!--Buttons-->
  
    <form method="GET" action="/reports/OutstandingPropertiesForMunicipality">
            <div class="form-group">
                <input type="hidden" name="municipalityId" value=""/>
                <label style="visibility: hidden">Move down</label> <!-- used to move the buttons down to match other inputs -->
                <button type="submit" class="btn btn-default btn-block">Print List</button>
            </div>
        </form>

        <form method="GET" action="/subsequents/properties">
            <div class="form-group">
                <input type="hidden" name="municipalityId" value=""/>
                <label style="visibility: hidden">Move Down</label> <!-- used to move the buttons down to match other inputs -->
                <button type="submit" class="btn btn-default btn-block">Pay Subsequents</button>
            </div>
        </form>
    </div>

</div>
@section scripts {
    <script src="~/Scripts/TaxLienScripts.js" type="text/javascript"></script>
}


